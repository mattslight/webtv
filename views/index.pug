extends layout

block content
  #wrapper
    .overlay
    nav#sidebar-wrapper.navbar.navbar-inverse.navbar-fixed-top(role='navigation')
      ul.nav.sidebar-nav
        li.sidebar-brand
          a(href='#') WebTV
        each category in categories
          li
            a(href='#' + category['categoryId']) #{category['categoryName']}
    #page-content-wrapper
      button.hamburger.is-closed(type='button', data-toggle='offcanvas')
        span.hamb-top
        span.hamb-middle
        span.hamb-bottom
      .container-fluid
        .row
          #jwplayer
          script(type='text/javaScript').
            var playerInstance = jwplayer("jwplayer");
          script(src='//content.jwplatform.com/players/g7fKzJsZ-Hfok2Nah.js')